{
  "expo": {
    "name": "Road Trip Bingo",
    "slug": "roadtripbingo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "roadtripbingo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "uk.co.iconnectit.roadtripbingo",
      "appleTeamId": "QHS5TSHJA5",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "uk.co.iconnectit.roadtripbingo",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "cssModules": false
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "ios": {
            "backgroundColor": "#1a1a2e",
            "image": "./assets/images/splash-icon-light.png",
            "resizeMode": "contain",
            "imageWidth": 200
          },
          "android": {
            "backgroundColor": "#1a1a2e",
            "image": "./assets/images/splash-icon.png",
            "resizeMode": "contain",
            "imageWidth": 200
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Road Trip Bingo to access your camera to snap and share photos of your road trip discoveries and memories."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Road Trip Bingo to save your road trip memories and discoveries to your device.",
          "savePhotosPermission": "Allow Road Trip Bingo to save photos of your road trip finds to your photo library.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      "react-native-edge-to-edge"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "504bc6cb-fb94-441b-8bfc-021edb506752"
      }
    },
    "owner": "mrzang77",
    "runtimeVersion": "1.0.0",
  }
}
