{
  "expo": {
    "name": "Dead Ahead",
    "slug": "deadahead",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "deadaheadbingo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "uk.co.iconnectit.deadahead",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "uk.co.iconnectit.deadahead",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "cssModules": false
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "ios": {
            "backgroundColor": "#1a1a2e",
            "image": "./assets/images/splash-icon-light.png",
            "resizeMode": "contain",
            "imageWidth": 200
          },
          "android": {
            "backgroundColor": "#1a1a2e",
            "image": "./assets/images/splash-icon.png",
            "resizeMode": "contain",
            "imageWidth": 200
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Dead Ahead Bingo to access your camera to capture roadkill evidence for the leaderboard."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow Dead Ahead Bingo to save your roadkill photos to your device.",
          "savePhotosPermission": "Allow Dead Ahead Bingo to save photos to your photo library.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      "react-native-edge-to-edge"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "a8837a6b-eacb-4282-b149-c278dface0fe"
      }
    },
    "owner": "mrzang77"
  }
}
